{
	"name": "builtbywill",
	"version": "2.0.0",
	"private": true,
	"homepage": "https://builtbywill.com",
	"scripts": {
		"analyze": "source-map-explorer build/static/js/main.*",
		"build": "react-scripts build",
		"coverage": "yarn test --coverage --watchAll=false",
		"coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
		"predeploy": "yarn build",
		"deploy": "gh-pages -b master -d build -m",
		"fix": "run-s fix:**",
		"fix:code": "yarn lint:code --fix",
		"fix:styles": "yarn lint:styles --fix",
		"lint": "run-s lint:**",
		"lint-code-core": "eslint --ext .js,.jsx,.ts,.tsx",
		"lint:code": "yarn lint-code-core .",
		"lint:styles": "stylelint \"src/**/*.css\"",
		"start": "react-scripts start",
		"test": "react-scripts test",
		"test:ci": "cross-env CI=true yarn test"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "npm run test:ci"
		}
	},
	"lint-staged": {
		"**/*.(j|t)s?(x)": [
			"yarn lint-code-core --fix"
		],
		"*.css": [
			"stylelint --fix"
		],
		"package.json": [
			"sort-package-json"
		]
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"eslintConfig": {
		"extends": "studiokit/react",
		"overrides": [
			{
				"files": [
					"**/*.ts?(x)"
				],
				"parser": "@typescript-eslint/parser",
				"parserOptions": {
					"project": "tsconfig.json"
				}
			}
		],
		"ignorePatterns": [
			"build/",
			"coverage/"
		]
	},
	"stylelint": {
		"extends": "stylelint-config-studiokit"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{ts,tsx,js,jsx}",
			"!src/index.tsx",
			"!src/utils/serviceWorker.ts"
		]
	},
	"resolutions": {
		"@types/react": "^16.9.22",
		"react-side-effect": "^2.1.0"
	},
	"dependencies": {
		"@types/react": "^16.9.22",
		"@types/react-dom": "^16.9.5",
		"@types/react-helmet": "^5.0.15",
		"@types/react-lazyload": "^2.6.0",
		"@types/react-router": "^5.1.4",
		"@types/react-router-dom": "^5.1.3",
		"core-js": "^3.6.4",
		"history": "^4.10.1",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-ga": "^2.7.0",
		"react-helmet": "^5.2.1",
		"react-lazyload": "^2.6.5",
		"react-loader-spinner": "^3.1.5",
		"react-router": "^5.1.2",
		"react-router-dom": "^5.1.2",
		"tachyons": "^4.11.1"
	},
	"devDependencies": {
		"@types/enzyme": "^3.10.5",
		"@types/enzyme-adapter-react-16": "^1.0.6",
		"@types/jest": "^24.9.0",
		"@typescript-eslint/eslint-plugin": "^2.20.0",
		"@typescript-eslint/parser": "^2.20.0",
		"babel-eslint": "^10.0.3",
		"coveralls": "^3.0.9",
		"cross-env": "^7.0.0",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-config-react-app": "^5.2.0",
		"eslint-config-studiokit": "purdue-tlt/eslint-config-studiokit.git#feature/typescript",
		"eslint-plugin-flowtype": "^3.1.2",
		"eslint-plugin-import": "^2.20.1",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.18.3",
		"eslint-plugin-react-hooks": "^1.7.0",
		"gh-pages": "^2.2.0",
		"husky": "^4.2.3",
		"lint-staged": "^10.0.7",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.19.1",
		"react-scripts": "^3.4.0",
		"react-test-renderer": "^16.12.0",
		"sort-package-json": "^1.40.0",
		"source-map-explorer": "^2.3.1",
		"stylelint": "^13.2.0",
		"stylelint-config-standard": "^20.0.0",
		"stylelint-config-studiokit": "purdue-tlt/stylelint-config-studiokit#feature/eslint-and-updates",
		"typescript": "^3.7.5"
	}
}
